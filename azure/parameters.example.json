{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "namePrefix": {
      "value": "staffsrv"
    },
    "location": {
      "value": "eastus"
    },
    "containerImage": {
      "value": "staff-server:latest"
    },
    "containerPort": {
      "value": 8080
    },
    "databaseUrl": {
      "value": "<YOUR_DATABASE_URL>"
    },
    "jwtSecret": {
      "value": "<GENERATE_A_SECRET>"
    },
    "nodeEnv": {
      "value": "production"
    },
    "acrSku": {
      "value": "Standard"
    },
    "appServiceSku": {
      "value": "P1v3"
    }
  }
}
