{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "error.json",
  "type": "object",
  "additionalProperties": true,
  "required": [
    "code"
  ],
  "properties": {
    "code": {
      "type": "string",
      "minLength": 1
    },
    "message": {
      "type": "string",
      "minLength": 1
    },
    "requestId": {
      "type": "string"
    }
  }
}
